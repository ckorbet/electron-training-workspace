{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "[VIDEOINFO] Debug",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/videoinfo",
        "runtimeExecutable": "${workspaceFolder}/videoinfo/node_modules/.bin/electron",
        "windows": {
          "runtimeExecutable": "${workspaceFolder}/videoinfo/node_modules/.bin/electron.cmd"
        },
        "args" : ["."],
        "outputCapture": "std",
        "protocol": "inspector",
        "stopOnEntry": false,
        "runtimeArgs": [
            "--nolazy"
        ],
        "console": "integratedTerminal",
        "sourceMaps": true,
        "localRoot": "${workspaceFolder}/to-dos",
        "remoteRoot": "${workspaceFolder}/to-dos",
        "outFiles": []
      }, 
